{
  "name": "wordle-wthout-limits",
  "description": "A fork of the popular word game Wordle, allowing unlimited play",
  "author": "Jayson Knight <info@jaysonknight.com> (https://jaysonknight.com)",
  "version": "0.8.0",
  "private": true,
  "dependencies": {
    "@azure/functions": "^3.0.0",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.2.3",
    "grapheme-splitter": "^1.0.4",
    "inversify": "^6.0.1",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.6.2",
    "ua-parser-js": "^1.0.33",
    "unirest": "^0.6.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "coverage": "npm test -- --coverage",
    "docs": "npx typedoc src/",
    "eject": "react-scripts eject",
    "fix": "prettier --write src",
    "lint": "prettier --check src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "serve": "serve -s build",
    "azure-sim": "swa start ./public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.23"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx,css,md}": "prettier --write"
  }
}
